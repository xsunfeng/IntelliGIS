      </item>
      <item>
        their inside parts  <tag> out = "partial"; </tag>
      </item>
      <item>
        partially <tag> out = "partial"; </tag>
      </item>
      <item>
        fully <tag> out = "whole"; </tag>
      </item>
      <item>
        partially inside <tag> out = "partial"; </tag>
      </item>
      <item>
        fully inside <tag> out = "whole"; </tag>
