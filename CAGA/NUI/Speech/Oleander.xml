  <rule id="modify_previous_answer">
    <example> They are parcels </example>
    <one-of>
      <item>
        <item>
          <ruleref uri="#negative"></ruleref>
        </item>
        <item repeat="0-1">
          <ruleref uri="#article"></ruleref>
        </item>
        <one-of>
          <item>
            distance
          </item>
          <item>
            speed limit
          </item>
          <item>
            time limit
          </item>
        </one-of>
        should be
        <one-of>
          <item>
            <ruleref uri="#distance"/>
            <tag> out.distance = rules.distance; </tag>
          </item>
          <item>
            <ruleref uri="#speed_limit"/>
            <tag> out.speed_limit = rules.speed_limit; </tag>
          </item>
          <item>
            <ruleref uri="#time_limit"/>
            <tag> out.time_limit = rules.time_limit; </tag>
          </item>
        </one-of>
      </item>

      <item>
        How about
        <one-of>
          <item>
            <ruleref uri="#distance"/>
            <tag> out.distance = rules.distance; </tag>
          </item>
          <item>
            <ruleref uri="#speed_limit"/>
            <tag> out.speed_limit = rules.speed_limit; </tag>
          </item>
          <item>
            <ruleref uri="#time_limit"/>
            <tag> out.time_limit = rules.time_limit; </tag>
          </item>
        </one-of>
      </item>

      <item>
        Can you make them larger
        <tag> out.symbol_size = "true"; </tag>
      </item>
      <item>
        Can you make them smaller
        <tag> out.symbol_size = "false"; </tag>
      </item>

    </one-of>
    <tag> out.correction = "true"; </tag>
  </rule>


