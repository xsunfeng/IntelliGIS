  <rule id="perform_bufferop" scope="public">
    <example> buffer operation </example>
    <example> I would like to explore the Parcels within 2 kilometers from fire stations </example>
    <item>
      <one-of>
        <item> buffer operation </item>
        <item> perform buffering </item>
        <item>
          <item repeat ="0-1">
            I would like to
          </item>
          explore the
          <item>
            <ruleref uri="#feature_class"/>
            <tag> out.feature_class = rules.feature_class;</tag>
          </item>
          within
          <one-of>
            <item>
              <ruleref uri="#distance"/>
              <tag> out.distance = rules.distance; </tag>
            </item>
            <item>
              <ruleref uri="#time_limit"/>
              <tag> out.time_limit = rules.time_limit; </tag>
            </item>
          </one-of>
          from
          <item>
            <ruleref uri="#source_layer"/>
            <tag> out.source_layer = rules.source_layer; </tag>
          </item>
        </item>

        <item>
          <item repeat ="0-1">
            I would like to
          </item>
          explore
          <item>
            <ruleref uri="#time_limit"/>
            <tag> out.time_limit = rules.time_limit;</tag>
          </item>
          around
          <item>
            <ruleref uri="#source_layer"/>
            <tag> out.source_layer = rules.source_layer; </tag>
          </item>
        </item>

        <item>
          explore
          <item repeat="0-1">
            <ruleref uri="#distance"/>
            <tag> out.distance = rules.distance; </tag>
          </item>
          <item>
            <one-of>
              <item> surrounding </item>
              <item> around </item>
            </one-of>
          </item>
          <item> around </item>
          <item repeat="0-1">
            <ruleref uri="#article"/>
          </item>
          <item>
            <ruleref uri="#source_layer"/>
            <tag> out.source_layer = rules.source_layer; </tag>
          </item>
        </item>
      </one-of>
    </item>
    <tag> out.intention = "Specify Region By Buffer"; </tag>
  </rule>

